<?xml version="1.0"?>
<robot name="cylinder_robot">
  
  <!-- Link for the cylinder -->
  <link name="base_link">
    <inertial>
    <mass value="25" />
    <origin xyz="0 0 0" rpy="0 0 0" />
    <inertia
         ixx="0.5781"
         ixy="0"
         ixz="0"
         iyy="0.5781"
         iyz="0"
         izz="0.78125" />
    </inertial>

    <visual>
      <geometry>
        <cylinder radius="0.25" length="0.3"/>
      </geometry>

      <material name="dark_blue">
        <color rgba="0 0 0.5 1"/>
      </material>

    </visual>

    <!-- Collision representation -->
    <collision>
      <origin xyz="0.5 0 0" rpy="0 0 0" />

      <geometry>
        <cylinder radius="0.25" length="0.3"/>
      </geometry>

    </collision> 
  </link>

  <!-- front_caster_link -->
  <link name="front_caster">
    <inertial>
      <mass value="0.25"/>
      <inertia ixx="6.25e-05" ixy="0" ixz="0" iyy="6.25e-05" iyz="0" izz="6.25e-05"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.025"/> 
      </geometry>
      <material name="Grey">
        <color rgba="0.6 0.6 0.6 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
    </collision>
  </link>

  <!-- back_caster_link -->
  <link name="back_caster">
    <inertial>
      <mass value="0.25"/>
      <inertia ixx="6.25e-05" ixy="0" ixz="0" iyy="6.25e-05" iyz="0" izz="6.25e-05"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.025"/> 
      </geometry>
      <material name="Grey">
        <color rgba="0.6 0.6 0.6 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
    </collision>
  </link>

  <!-- right_wheel -->
  <link name="right_wheel">
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2.0"/>
        <inertia ixx="0.0025" ixy="0" ixz="0" iyy="0.0025" iyz="0" izz="0.00125"/>
    </inertial>

    <visual>
        <origin xyz="0 0 0" rpy="1.5707 0 0"/>
        <geometry>
            <cylinder radius="0.026" length="0.1"/> 
        </geometry>
        <material name="red">
            <color rgba="1 0 0 1"/>
        </material>    
    </visual>

    <collision>
        <origin xyz="0 0 0" rpy="1.5707 0 0"/>
        <geometry>
            <cylinder radius="0.026" length="0.1"/> 
        </geometry>
    </collision>

  </link>

  <!-- left_wheel -->
  <link name="left_wheel">
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2.0"/>
        <inertia ixx="0.0025" ixy="0" ixz="0" iyy="0.0025" iyz="0" izz="0.00125"/>
    </inertial>

    <visual>
        <origin xyz="0 0 0" rpy="1.5707 0 0"/>
        <geometry>
            <cylinder radius="0.026" length="0.1"/> 
        </geometry>
        <material name="red">
            <color rgba="1 0 0 1"/>
        </material>
    </visual>

    <collision>
        <origin xyz="0 0 0" rpy="1.5707 0 0"/>
        <geometry>
            <cylinder radius="0.026" length="0.1"/> 
        </geometry>
    </collision>
  </link> 

    <!-- base_footprint_link -->
  <link name="base_footprint">
    <inertial>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <mass value="0.001"/>
    <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <box size="0.01 0.01 0.01"/> 
        </geometry>
        <material name="transparent">
            <color rgba="0 0 0 0"/> 
        </material>
    </visual>
    <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <box size="0.01 0.01 0.01"/>
        </geometry>
    </collision>
  </link>

  <!-- front_laser_link -->
  <link name="front_laser">
    <inertial>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <mass value="0.1"/> 
    <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/> 
    </inertial>
    <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
        <box size="0.1 0.02 0.02"/>
        </geometry>
    </collision>
  </link>

  <!-- right_wheel_joint  -->
  <joint name="right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <origin xyz="0.0 -0.2 -0.15" rpy="0 0 0"/> 
    <axis xyz="0 1 0"/>
    <limit effort="10000" velocity="500" />
    <joint_properties damping="1.0" friction="1.0" /> 
  </joint>

  <!-- left_wheel_joint  -->
  <joint name="left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <origin xyz="0.0 0.2 -0.15" rpy="0 0 0"/> 
    <axis xyz="0 1 0"/>
    <limit effort="10000" velocity="500" />
    <joint_properties damping="1.0" friction="1.0" /> 
    </joint>

  <!-- base_footprint_joint  --> 
  <joint name="base_footprint_joint" type="fixed">
    <parent link="base_link"/>
    <child link="base_footprint"/>
    <origin xyz="0 0 -0.15" rpy="0 0 0"/> 
  </joint>

  <!-- front_caster_joint  --> 
  <joint name="front_caster_joint" type="fixed">
    <parent link="base_link"/>
    <child link="front_caster"/>
    <origin xyz="0.1 0.0 -0.15" rpy="0 0 0"/> 
    <limit effort="10000" velocity="1000" />
    <joint_properties damping="1.0" friction="0" /> 
  </joint>
  <!-- back_caster_joint  --> 
  <joint name="back_caster_joint" type="fixed">
    <parent link="base_link"/>
    <child link="back_caster"/>
    <origin xyz="-0.1 0.0 -0.15" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000" />
    <joint_properties damping="1.0" friction="0" /> 
  </joint>
  <!-- front_laser_joint  --> 
  <joint name="front_laser_joint" type="fixed">
  <parent link="base_link"/>
  <child link="front_laser"/>
  <origin xyz="0.25 0.0 0.075" rpy="3.14159 0 0"/> 
  </joint>

</robot>